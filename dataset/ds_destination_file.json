{
	"name": "ds_destination_file",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_source_blob_sa",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"dsd_SheetName": {
				"type": "string",
				"defaultValue": "asd"
			}
		},
		"folder": {
			"name": "OldSets"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().dsd_SheetName,'.csv')",
					"type": "Expression"
				},
				"container": "archive"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}